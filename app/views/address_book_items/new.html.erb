<h1>New address_book_item</h1>

<% form_for(@address_book_item) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :address_book_id %><br />
    <%= f.text_field :address_book_id %>
  </p>
  <p>
    <%= f.label :item %><br />
    <%= f.text_field :item %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', address_book_items_path %>