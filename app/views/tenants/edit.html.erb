<h1>Editing tenant</h1>

<% form_for(@tenant) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :location_id %><br />
    <%= f.text_field :location_id %>
  </p>
  <p>
    <%= f.label :staff_id %><br />
    <%= f.text_field :staff_id %>
  </p>
  <p>
    <%= f.label :student_id %><br />
    <%= f.text_field :student_id %>
  </p>
  <p>
    <%= f.label :keyaccept %><br />
    <%= f.date_select :keyaccept %>
  </p>
  <p>
    <%= f.label :keyexpectedreturn %><br />
    <%= f.date_select :keyexpectedreturn %>
  </p>
  <p>
    <%= f.label :keyreturned %><br />
    <%= f.date_select :keyreturned %>
  </p>
  <p>
    <%= f.label :force_vacate %><br />
    <%= f.check_box :force_vacate %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @tenant %> |
<%= link_to 'Back', tenants_path %>