<div class="tform">
<table id="form" width=80%>
	<tr>
		<th colspan = 2>New Message</th>
	</tr>
	
	
	<tr>
		<td class="leftcol"><label for="to_id">To :</label></td>
		<td><%= f.collection_select :to_id, Staff.find(:all), :id, :mykad_with_staff_name, :include_blank => true %></td>
	</tr>
	
	<tr>
		<td class="leftcol"><label for="message">Message:</label></td>
		<td><%= f.text_area :message %></td>
	</tr>
	
</table>
<BR>
</div>