<h1>Listing intakes</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Register on</th>
    <th>Programme</th>
    <th>Is active</th>
  </tr>

<% @intakes.each do |intake| %>
  <tr>
    <td><%=h intake.name %></td>
    <td><%=h intake.description %></td>
    <td><%=h intake.register_on %></td>
    <td><%=h intake.programme_id %></td>
    <td><%=h intake.is_active %></td>
    <td><%= link_to 'Show', intake %></td>
    <td><%= link_to 'Edit', edit_intake_path(intake) %></td>
    <td><%= link_to 'Destroy', intake, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New intake', new_intake_path %>